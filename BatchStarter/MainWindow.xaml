        <Button Content="종료" Command="{Binding CloseCmd}" CommandParameter="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type Window}}}" Width="100" Height="30" Margin="5, 0" />
    </Grid>
</Window>
